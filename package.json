  "devDependencies": {
    "@sensebox/eslint-config-sensebox": "^1.0.0",
    "chai": "^4.1.0",
    "eslint": "^8.49.0",
    "mocha": "^10.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "@grpc/proto-loader": "^0.7.0",
    "@sensebox/opensensemap-api-models": "2.0.3",
    "@sensebox/osem-protos": "^1.1.0",
    "mqtt": "^4.2.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
